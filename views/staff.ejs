<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Staff &mdash; MineRIP Network</title>
    <meta name="theme-color" content="#FFD700" />
    <meta property="og:image" content="https://cdn.discordapp.com/attachments/871795400225480734/900590560853774346/logo_png_1.png">
    <link rel="icon" type="image/png" sizes="1500x1500" href="https://cdn.discordapp.com/attachments/871795400225480734/900590560853774346/logo_png_1.png">
    <meta name="title" content="MineRIP ┃ Staff">
    <meta name="og:title" content="MineRIP ┃ Staff">
    <meta name="twitter:title" content="MineRIP ┃ Staff">   
    <meta name="description" content="The staff that allow us to keep running the Network." />
    <meta property="og:description" content="The staff that allow us to keep running the Network." />
    <meta property="twitter:description" content="The staff that allow us to keep running the Network." />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bulma@0.9.2/css/bulma.min.css">
    <link rel="stylesheet" href="/css/global.css">
    <link rel="stylesheet" href="/css/staff.css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="/css/flash.css" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pace/1.2.3/pace.min.js"></script>
</head>
<body>
<%- include('./partials/Navbar.ejs') %>

<div class="container content" style="height: 100%;">
    <h1 class="title is-1">Staff</h1>
    <p class="subtitle">The people who keep the server running.</p>
    <hr>
    <ul>
    <% for (var index in staffRanks) { %>
        <div class="box staff-section">
            <div class="block bg-<%= staffRanks[index].color %> <%= staffRanks[index].italics %>">
                <p class="subtitle shittyshit has-text-centered tcolor-<%= staffRanks[index].color %>" style="padding-top: 9px; font-weight: 700 !important;"><%= staffRanks[index].name.toUpperCase() %></p>
            </div>
            <center>
            <div class="columns">
                <% for (var user in users) { %>
                    <% if (users[user].rank == staffRanks[index].id) { %>
                    <div class="column">
                        <img draggable="false" style="width: 65px;" src="https://visage.surgeplay.com/full/<%= users[user].uuid %>" alt="shit">
                        <br> <a class="<%= staffRanks[index].color %> <%= staffRanks[index].italics %>" href="/profile/<%= users[user].username %>"><%= users[user].username %></a>
                    </div>
                    <% } %>
                <% } %>
            </div>
            </center>
        </div>
    <% } %>
    </ul>
</div>
<br>

<script defer src="/js/ModalHelper.js"></script>
<script defer src="https://use.fontawesome.com/releases/v5.3.1/js/all.js"></script>
<!-- ## yuri -->

<%- include('./partials/Footer.ejs') %>

</body>
</html>